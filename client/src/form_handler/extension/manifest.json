{
  "manifest_version": 3,
  "name": "Form Analyzer",
  "version": "1.0",
  "description": "Analyzes and interacts with forms on web pages",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_title": "Form Analyzer"
  }
} 